{
  "name": "senler-sdk",
  "version": "2.0.2",
  "description": "Официальный TypeScript SDK для работы с API Senler",
  "author": {
    "name": "Senler.ru",
    "url": "https://senler.ru"
  },
  "contributors": [
    {
      "name": "SenlerBot",
      "email": "info@senler.ru",
      "url": "https://github.com/SenlerBot"
    }
  ],
  "homepage": "https://github.com/SenlerBot/senler-sdk#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/SenlerBot/senler-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/SenlerBot/senler-sdk/issues",
    "email": "support@senler.ru"
  },
  "license": "MIT",
  "sideEffects": false,
  "keywords": [
    "senler",
    "sdk",
    "api",
    "vk",
    "vkontakte",
    "marketing",
    "automation",
    "typescript",
    "javascript",
    "bot",
    "messenger",
    "chatbot",
    "subscribers",
    "mailings",
    "utm",
    "analytics"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "npm@9.0.0",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "TYPES_USAGE_EXAMPLE.md",
    "API_REFERENCE.md"
  ],
  "directories": {
    "lib": "dist",
    "test": "tests",
    "doc": "docs"
  },
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.m.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.m.js",
      "require": "./dist/index.js",
      "browser": "./dist/index.umd.js"
    },
    "./package.json": "./package.json"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "build": "microbundle --no-compress --sourcemap --tsconfig build.tsconfig.json",
    "build:clean": "npm run build:remove-dist && npm run build",
    "build:remove-dist": "if exist dist (rmdir /s /q dist)",
    "build:watch": "microbundle watch",
    "dev": "npm run build:watch",
    "lint": "eslint --fix",
    "lint:check": "eslint src tests --ext .ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run type-check && npm run lint:check && npm run test && npm run build",
    "prepack": "npm run build",
    "release": "npm run prepublishOnly && npm publish"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.10",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "express": "^4.21.0",
    "jest": "^29.7.0",
    "microbundle": "^0.15.1",
    "nock": "^13.5.5",
    "pino-pretty": "^11.2.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@types/cache-manager": "^4.0.6",
    "axios": "^1.7.7",
    "axios-retry": "^4.5.0",
    "cache-manager": "^6.1.0",
    "joi": "^17.13.3",
    "pino": "^9.4.0"
  }
}
